<?xml version='1.0' encoding='utf-8'?>
<widget id="diamonds.movil.app" version="3.3.2" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Diamonds App</name>
    <description>
        Aplicaci√≥n Movil de Diamonds
    </description>
    <author email="yemosingle@gmail.com" >
        Diamonds
    </author>
    <content src="index.html" />
    <access origin="*" />
    <platform name="android">
        <icon density="ldpi" src="www/res/icon/android/drawable-ldpi-icon.png" />
